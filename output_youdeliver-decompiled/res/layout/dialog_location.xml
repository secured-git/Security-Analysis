<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout android:background="@drawable/top" android:layout_width="fill_parent" android:layout_height="@dimen/list_title_size">
        <TextView android:textSize="@dimen/list_title_text_size" android:textColor="@android:color/white" android:gravity="center_vertical" android:padding="4.0dip" android:layout_width="fill_parent" android:layout_height="@dimen/list_title_size" android:text="@string/orderRepeat" />
    </RelativeLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/list_title_text_size" android:gravity="center_vertical" android:padding="4.0dip" android:layout_width="fill_parent" android:layout_height="@dimen/list_title_size" android:text="@string/addressForRepeat" />
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:gravity="center_vertical" android:background="@drawable/text_field_mid" android:layout_width="wrap_content" android:layout_height="fill_parent" android:text="@string/searchStreet" style="@style/PaddingLRText" />
            <TextView android:enabled="false" android:id="@id/DeliverStreet" android:background="@drawable/edit_text_mid" android:layout_width="fill_parent" android:layout_height="fill_parent" android:lines="1" style="@style/PaddingLRText" />
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:gravity="center_vertical" android:background="@drawable/text_field_mid" android:layout_width="wrap_content" android:layout_height="fill_parent" android:text="@string/searchNumber" style="@style/PaddingLRText" />
            <TextView android:enabled="false" android:id="@id/DeliverNumber" android:background="@drawable/edit_text_mid" android:layout_width="fill_parent" android:layout_height="fill_parent" android:lines="1" style="@style/PaddingLRText" />
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:gravity="center_vertical" android:background="@drawable/text_field_mid" android:layout_width="wrap_content" android:layout_height="fill_parent" android:text="@string/searchSuggestion" style="@style/PaddingLRText" />
            <TextView android:enabled="false" android:id="@id/DeliverZipCode" android:background="@drawable/edit_text_mid" android:layout_width="fill_parent" android:layout_height="fill_parent" android:lines="1" style="@style/PaddingLRText" />
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:gravity="center_vertical" android:background="@drawable/text_field_mid" android:layout_width="wrap_content" android:layout_height="fill_parent" android:text="@string/searchCity" style="@style/PaddingLRText" />
            <TextView android:enabled="false" android:id="@id/DeliverCity" android:background="@drawable/edit_text_mid" android:layout_width="fill_parent" android:layout_height="fill_parent" android:lines="1" style="@style/PaddingLRText" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>